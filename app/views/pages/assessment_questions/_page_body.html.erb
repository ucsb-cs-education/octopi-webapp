<% unless @assessment_question.assessment_questions.blank? %>
    <% @assessment_question.assessment_questions.each { |aq| %>
        <div style="display:inline-block">
          <%= button_to aq.title, assessment_question_path(aq), :method => :get %>
        </div>
    <% } %>
<% end %>
<br>
<div id="tabs">
  <ul>
    <li><a href="#question-body">Question</a></li>
  </ul>
  <div id="question-body" <%= "class=octopieditable" if can? :update, @page %>>
    <%= @assessment_question.question_body.html_safe if @assessment_question.question_body.present? %>
  </div>

</div>