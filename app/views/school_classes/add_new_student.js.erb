$(function() {
    $("#classlist").append('<tr><td><%= @student.first_name %></td><td><%= @student.last_name %></td><td><%= @student.login_name %></td></tr>');
    $("#create_and_add_student_form").hide()
    $("#toggle_add_new_student_button").show()
    $("#cancel_button").hide()
    $('#create_and_add_student_form')[0].reset()
});