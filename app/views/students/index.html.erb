<% provide(:title, "Students at #{@school.name}") %>
<% breadcrumb :staff_school_students, @school %>
<div class="well well-sm">
  <% @school.students.not_teststudents.each do |student| %>
      <p><%= link_to "#{student.name} (#{student.login_name})", student_path(:id => student.id) %></p>
  <% end %>
</div>
<p>
  <%= link_to 'Add a new student', new_school_student_path(@school), class: 'btn btn-default' %>
</p>


